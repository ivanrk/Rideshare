@model IEnumerable<CategoryListingModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

@foreach (var item in Model)
{
    <p>@item.Name</p>
    @foreach (var subforum in item.Subforums)
    {
        <p>@subforum.Name</p>
    }
    <p>--------------------------</p>
}

